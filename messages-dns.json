{
  "messages": [
    {
        "wasm_file": "wasm_erc20_code/erc20.wasm",
        "type": "init", "contract_addr": "erc20_contract_addr", "sender": "new_owner_addr",
        "message": {"name":"Test eth token","symbol":"ETH","decimals":10,"initial_balances":[{"address":"new_owner_addr","amount":"10000"}]}
    },
    {
      "wasm_file": "wasm_dns_code/dns.wasm",
      "type": "init", "contract_addr": "dns_contract_addr", "sender": "original_owner_addr",
      "message": {"erc20":"erc20_contract_addr"}
    },
    {
      "type": "handle", "contract_addr": "dns_contract_addr", "sender": "original_owner_addr",
      "message": {"register_domain":{"domain":"www.cosmos.com"}}
    },
    {
      "type": "query", "contract_addr": "dns_contract_addr",
      "message": {"get_owner":{"domain":"www.cosmos.com"}}
    },
    {
      "type": "handle", "contract_addr": "erc20_contract_addr", "sender": "new_owner_addr",
      "message": {"approve":{"spender":"dns_contract_addr","amount":"600"}}
    },
    {
      "type": "query", "contract_addr": "erc20_contract_addr",
      "message": {"allowance":{"owner":"new_owner_addr", "spender": "dns_contract_addr"}}
    },
    {
      "type": "handle", "contract_addr": "dns_contract_addr", "sender": "original_owner_addr",
      "message": {"sell_domain":{"buyer":"new_owner_addr","domain":"www.cosmos.com"}}
    },
    {
      "type": "query", "contract_addr": "dns_contract_addr",
      "message": {"get_owner":{"domain":"www.cosmos.com"}}
    },
    {
      "type": "query", "contract_addr": "erc20_contract_addr",
      "message": {"allowance":{"owner":"new_owner_addr", "spender": "dns_contract_addr"}}
    }
  ]
}